<?xml version="1.0" encoding="utf-8"?>
<resources>
	<declare-styleable name="ToothyProgress">
		<attr name="strokeLineCapProgress" format="enum">
			<enum name="butt" value="0"/>
			<enum name="round" value="1"/>
			<enum name="square" value="2"/>
		</attr>
		<attr name="strokeLineCapProgressBackground" format="enum">
			<enum name="butt" value="0"/>
			<enum name="round" value="1"/>
			<enum name="square" value="2"/>
		</attr>
		<attr name="strokeLineCapTrack" format="enum">
			<enum name="butt" value="0"/>
			<enum name="round" value="1"/>
			<enum name="square" value="2"/>
		</attr>
		<attr name="progress" format="float"/>
		<attr name="progressColor" format="reference|color"/>
		<attr name="progressBackgroundColor" format="reference|color"/>
		<attr name="trackColor" format="reference|color"/>
		<attr name="progressWidth" format="dimension"/>
		<attr name="trackWidth" format="dimension"/>
		<attr name="progressBackgroundWidth" format="dimension"/>
		<attr name="isBuilderMode" format="boolean"/>
		<attr name="progressType" format="enum">
			<enum name="determinate" value="0"/>
			<enum name="indeterminate" value="1"/>
		</attr>
		<attr name="indeterminateTrackSize" format="float"/>
	</declare-styleable>
</resources>